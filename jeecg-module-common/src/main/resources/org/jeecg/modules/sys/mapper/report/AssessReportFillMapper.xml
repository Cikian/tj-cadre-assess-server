<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.sys.mapper.report.AssessReportFillMapper">

    <select id="getAllItems" parameterType="java.lang.String"
            resultType="org.jeecg.modules.sys.dto.ReportFieldDTO">
        SELECT current_year, assess_name, depart, status, deadline
        FROM assess_report_fill
        WHERE current_year = #{currentYear}</select>

</mapper>