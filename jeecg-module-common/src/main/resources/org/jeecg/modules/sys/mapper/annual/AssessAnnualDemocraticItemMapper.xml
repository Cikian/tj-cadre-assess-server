<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.sys.mapper.annual.AssessAnnualDemocraticItemMapper">

    <delete id="deleteByMainId" parameterType="java.lang.String">
        DELETE
        FROM assess_annual_democratic_item
        WHERE config_id = #{mainId}
    </delete>

    <select id="selectByMainId" parameterType="java.lang.String"
            resultType="org.jeecg.modules.sys.entity.annual.AssessAnnualDemocraticItem">
        SELECT *
        FROM assess_annual_democratic_item
        WHERE config_id = #{mainId}
    </select>
    <select id="getLastVersionItemsByType" resultType="org.jeecg.modules.sys.entity.annual.AssessAnnualDemocraticItem">
        select item_name, score
        from assess_annual_democratic_item
        where config_id = (select id
                           from assess_annual_democratic_config
                           where type = #{type}
                           ORDER BY create_time DESC
                           LIMIT 1)
    </select>
</mapper>
