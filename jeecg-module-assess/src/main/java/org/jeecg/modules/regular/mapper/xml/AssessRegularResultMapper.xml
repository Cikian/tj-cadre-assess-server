<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.regular.mapper.AssessRegularResultMapper">

    <delete id="deleteByYear">
        delete
        from assess_regular_result
        where current_year = #{year}
    </delete>
</mapper>