<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.regular.mapper.AssessRegularReportMapper">

    <select id="getAllItems" parameterType="java.lang.String"
            resultType="org.jeecg.modules.regular.dto.RegularFieldDTO">
        SELECT assess_name, current_year, department_code, deadline, status
        FROM assess_regular_report
        WHERE current_year = #{currentYear}</select>

</mapper>